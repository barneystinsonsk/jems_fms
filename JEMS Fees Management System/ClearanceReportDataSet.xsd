                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select student_details.student_id, first_name, middle_name, last_name, father_name
from 
student_monthly_fees join student_details on 
student_monthly_fees.student_id = student_details.student_id where
month = 'MAR' and student_monthly_fees.receipt_id is not null
and student_category &lt;&gt; 'EXX' and 
(student_details.class = @class1 or student_details.class = @class2) and
(section = @sec1 or section = @sec2 or section = @sec3 or section = @sec4 or section = @sec5) and 
student_monthly_fees.session like @session
and student_monthly_fees.date &gt;= @date1 and student_monthly_fees.date &lt;= @date2
</CommandText>
