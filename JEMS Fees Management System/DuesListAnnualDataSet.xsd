                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select ROW_NUMBER() over(order by current_session) as row, student_details.student_id, first_name, middle_name, last_name, father_name, 
student_details.current_session, student_details.class,
	student_details.student_category, (school_dev + lab_dev + student_annual_fees.caution) as a_total
	
from student_details join student_annual_fees on (student_details.student_id = student_annual_fees.student_id
and student_details.current_session = student_annual_fees.session)
 where student_details.student_category &lt;&gt; 'EXX' and 
student_details.student_category &lt;&gt; 'PRV' and student_annual_fees.receipt_id is null and 
student_details.current_session like @session and (student_details.class = @Class1 or student_details.class = @Class2)  and (student_details.section = @sect1 or
student_details.section = @sect2 or student_details.section = @sect3 or student_details.section = @sect4 or
student_details.section = @sect5)


</CommandText>
