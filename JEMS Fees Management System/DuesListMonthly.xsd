                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>with Monthly_Due as


(select student_details.student_id, student_details.first_name, middle_name, last_name, father_name, 
student_details.current_session, student_monthly_fees.month,
	student_details.student_category, (tuition + management + smart_class + report_diary + sports + science + red_cross + guide + insurance +
	school_activities + computer + local_exam) as m_total, late_fees, student_monthly_fees.receipt_id,
student_details.mobile
from student_details join student_monthly_fees on (student_details.student_id = student_monthly_fees.student_id
and student_details.current_session = student_monthly_fees.session)
 where student_details.student_category &lt;&gt; 'EXX' and 
student_details.student_category &lt;&gt; 'PRV' and student_monthly_fees.receipt_id is null
and ((student_details.current_session = @currentSession
and dbo.sort_by_month(student_monthly_fees.month) &lt; @month )
or
(student_details.current_session &lt;&gt; @currentSession
and student_details.current_session is not null
)
)
and dbo.sort_by_month(student_monthly_fees.month) &lt;= dbo.sort_by_month(ISNULL(student_details.suspension_month, 'MAR'))
and (student_details.class = @Class1 or student_details.class = @Class2) and (student_details.section = @sect1 or
student_details.section = @sect2 or student_details.section = @sect3 or student_details.section = @sect4 or
student_details.section = @sect5)
)

select ROW_NUMBER() over(order by Monthly_Due.current_session, student_id) as row, 
student_id, first_name, middle_name, last_name, father_name, current_session, student_category, sum(m_total) as m_total,
ISNULL(sum(late_fees),0) as late_fees,
 dbo.monthName(min(dbo.sort_by_month(month))) as from_m,
 dbo.monthName(max(dbo.sort_by_month(month))) as to_m,
Monthly_Due.mobile as mobile
 from Monthly_Due where Monthly_Due.current_session like @session
 group by Monthly_Due.student_id, Monthly_Due.first_name, Monthly_Due.current_session,
 middle_name, last_name, father_name,
 student_category, Monthly_Due.mobile order by row</CommandText>
