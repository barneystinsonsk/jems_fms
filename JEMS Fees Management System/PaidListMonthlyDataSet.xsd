                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>with Monthly_Paid as

(
(select student_details.student_id, student_details.first_name, middle_name, last_name,
student_monthly_fees.session, student_monthly_fees.month,
	student_details.student_category, (tuition + management + smart_class + report_diary + sports + science + red_cross + guide + insurance +
	school_activities + computer + local_exam) as m_total, student_monthly_fees.receipt_id
from student_details join student_monthly_fees on (student_details.student_id = student_monthly_fees.student_id)
 where student_details.student_category &lt;&gt; 'EXX' and 
student_details.student_category &lt;&gt; 'PRV' and 
student_monthly_fees.date &gt;= @date1 and student_monthly_fees.date &lt;= @date2 and
student_monthly_fees.receipt_id is not null and 
student_monthly_fees.session like @session and (student_details.class = @Class1 or student_details.class = @Class2) and (student_details.section = @sect1 or
student_details.section = @sect2 or student_details.section = @sect3 or student_details.section = @sect4 or
student_details.section = @sect5)
)
)
select  student_id, first_name, middle_name, last_name, session, student_category, sum(m_total) as m_total,
 dbo.monthName(min(dbo.sort_by_month(month))) as from_m,
 dbo.monthName(max(dbo.sort_by_month(month))) as to_m
 from Monthly_Paid 
 group by Monthly_Paid.student_id, Monthly_Paid.first_name, Monthly_Paid.session,
 middle_name, last_name,
 student_category order by student_id</CommandText>
