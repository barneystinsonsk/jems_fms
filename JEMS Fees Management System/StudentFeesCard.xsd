                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>with Main as
(select top 12  tuition, management, smart_class, report_diary, sports, science, red_cross,
guide, insurance, school_activities, computer, local_exam, month, first_name, middle_name, last_name, student_details.class as class_code, student_monthly_fees.student_id as student_id,
student_monthly_fees.session as session, convert(varchar(10), GetDate(),120 ) as today
 from student_monthly_fees inner join student_details on student_monthly_fees.student_id = student_details.student_id
 where student_monthly_fees.student_id = @student_id and student_monthly_fees.session = @session
 order by student_monthly_fees.session desc)

select *, (Main.tuition + Main.management + Main.smart_class + Main.report_diary + Main.sports + Main.science + Main.red_cross + Main.guide + Main.insurance + Main.school_activities + Main.computer + Main.local_exam) as total, 
case
	when class_code in ('NUR') then 'NUR'
	when class_code in ('KG1') then 'KG 1'
	when class_code in ('KG2') then 'KG 2'
	when class_code in ('001') then 'I'
	when class_code in ('002') then 'II'
	when class_code in ('003') then 'III'
	when class_code in ('004') then 'IV'
	when class_code in ('005') then 'V'
	when class_code in ('006') then 'VI'
	when class_code in ('007') then 'VII'
	when class_code in ('008') then 'VIII'
	when class_code in ('009') then 'IX'
	when class_code in ('010') then 'X'
	when class_code in ('C1P') then 'XI COM'	
	when class_code in ('C1I') then 'XI IP'
	when class_code in ('S11') then 'XI SC'
	when class_code in ('C2P') then 'XII COM'	
	when class_code in ('C2I') then 'XII IP'
	when class_code in ('S12') then 'XII SC'
end
as theClass,
case 
	when Main.month = 'APR' then 'April'
	when Main.month = 'MAY' then 'May'
	when Main.month = 'JUN' then 'June'
	when Main.month = 'JUL' then 'July'
	when Main.month = 'AUG' then 'August'
	when Main.month = 'SEP' then 'September'
	when Main.month = 'OCT' then 'October'
	when Main.month = 'NOV' then 'November'
	when Main.month = 'DEC' then 'December'
	when Main.month = 'JAN' then 'January'
	when Main.month = 'FEB' then 'February'
	when Main.month = 'MAR' then 'March'
end as f_month
 from Main order by 
case 
	when Main.month = 'APR' then 1
	when Main.month = 'MAY' then 2
	when Main.month = 'JUN' then 3
	when Main.month = 'JUL' then 4
	when Main.month = 'AUG' then 5
	when Main.month = 'SEP' then 6
	when Main.month = 'OCT' then 7
	when Main.month = 'NOV' then 8
	when Main.month = 'DEC' then 9
	when Main.month = 'JAN' then 10
	when Main.month = 'FEB' then 11
	when Main.month = 'MAR' then 12
end asc</CommandText>
